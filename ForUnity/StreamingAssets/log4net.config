<log4net>
  <appender name="UnityAppender" type="UnityDebugAppender">
    <layout type="log4net.Layout.PatternLayout,log4net">
      <param name="ConversionPattern" value="%d{ABSOLUTE} %-5p %c{1}:%L - %m%n"/>
    </layout>
  </appender>
  <root>
    <level value="DEBUG"/>
    <appender-ref ref="UnityAppender"/>
  </root>
</log4net>